{
  "name": "chrome-plugin-demo",
  "version": "1.0",
  "description": "Build an Extension!",
  "manifest_version": 2,
  "chrome_url_overrides": {
    "newtab": "popup.html"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+F",
        "mac": "MacCtrl+Shift+F"
      },
      "description": "Opens popup.html"
    }
  },
    "background": {
      "scripts": ["background.js"],
      "persistent": true
    }
  }
