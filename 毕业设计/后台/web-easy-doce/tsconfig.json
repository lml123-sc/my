{
    "compilerOptions": {
        "target": "es6",
        "lib": ["es6", "dom"],
        "module": "esnext",
        "moduleResolution": "node",
        "jsx": "react",
        "sourceMap": true,
        "strict": true,
        "importHelpers": true,
        "skipLibCheck": true,
        "allowJs": true,
        "allowSyntheticDefaultImports": true,
        "noImplicitAny": false,
        "experimentalDecorators": true,
    }
}